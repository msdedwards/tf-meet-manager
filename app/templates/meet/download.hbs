<section class="section">
    <div class="container is-upscaled-mobile">
        <nav class="level is-mobile">
            <div class="level-left">
                <div class="level-item">
                    <h1 class="title no-print">Download Results</h1>
                </div>
            </div>
            <div class="level-right">
                <div class="level-item">
                    <div class="buttons">
                        <a class="button is-primary" href="data:text/csv;charset=utf-8,{{this.model}}"
                            download="meet-{{this.meetId}}-results.csv">
                            Download
                        </a>
                        <CopyButton class="button" @clipboardTarget="#csv">
                            Copy
                        </CopyButton>
                    </div>
                </div>
            </div>
        </nav>
        <div class="box is-paddingless">
            <pre><code id="csv">{{this.model}}</code></pre>
        </div>
    </div>
</section>